@model winerack.Models.Wine
<meta property="og:type" content="winerackio:wine">
<meta name="twitter:title" property="og:title" content="@Model.Vineyard.Name @Model.Description">
<meta property="winerackio:region" content="@Model.Region.Label">
<meta property="winerackio:varietal" content="@Model.Varietal.Name">
<meta property="winerackio:vineyard" content="@Model.Vineyard.Name">
<meta property="winerackio:vintage" content="@Html.DisplayFor(m => m.Vintage)">