@model IList<winerack.Models.User>

@if (Model.Count > 0) {
    <p>
        With
        @for (var i = 0; i < Model.Count; i++) {
            if (i > 0 && i == Model.Count - 1) {
                @Html.Raw(" and ")
            } else if (i > 0) {
                @Html.Raw(", ")
            }
            <a href="/user/@Model[i].UserName">@Model[i].Name</a>
        }
    </p>
}
